    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0"
  },
  "bizdp": {
    "npm": {
      "command": "npm",
      "registry": "http://registry.npm.sogou"
    },
    "build": "gulp",
    "server": {
      "command": "webpack-dev-server",
      "options": {
        "inline": true,
