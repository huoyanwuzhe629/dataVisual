        "host": "0.0.0.0",
        "colors": true,
        "progress": true,
        "display-modules": true
      }
    },
    "mock": {
      "surffix": ".action"
    },
    "validate": {
      "command": "eslint",
      "options": {},
      "src": "src"
    }
  },
  "globalDevDependencies": {
    "gulp": "*",
    "webpack-dev-server": "*",
    "eslint": "*"
  }
